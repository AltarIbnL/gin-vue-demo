{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport { mapState } from 'vuex';\nexport default {\n  computed: mapState({\n    // 从缓存中获取改为从vuex 中获取，因为vuex是响应式的，\n    userinfo: function userinfo(state) {\n      return state.userModule.userInfo;\n    } // return this.$store.state.userModule.userInfo;\n\n  })\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0CA;AACA;EACAA;IACA;IACAC;MAAA;IAAA,CAFA,CAGA;;EAHA;AADA","names":["computed","userinfo"],"sourceRoot":"src/views/layout","sources":["Navbar.vue"],"sourcesContent":["<template>\n  <div>\n    <b-navbar\n      toggleable=\"lg\"\n      type=\"dark\"\n      variant=\"info\"\n    >\n    <b-container>\n<!--      <li class=\"nav-item active\" @click=\"$router.replace({name:'home'})\">-->\n<!--        <a class=\"navbar-brand\" href=\"#\">U-Learn</a>-->\n<!--      </li>-->\n        <b-navbar-brand @click=\"$router.push({name: 'home'})\">U-Learn</b-navbar-brand>\n        <b-navbar-toggle target=\"nav-collapse\"></b-navbar-toggle>\n      <b-collapse\n        id=\"nav-collapse\"\n        is-nav\n      >\n      <b-navbar-nav class=\"ml-auto\">\n        <b-nav-item-dropdown\n          right\n          v-if=\"userinfo\"\n        >\n          <!-- Using 'button-content' slot -->\n          <template v-slot:button-content>\n            <em>{{userinfo.name}}</em>\n          </template>\n          <b-dropdown-item >个人主页</b-dropdown-item>\n          <b-dropdown-item>退出</b-dropdown-item>\n        </b-nav-item-dropdown>\n<!--        没有 userinfo才显示注册和登录-->\n        <div v-if=\"!userinfo\">\n<!--          在登陆页面不显示登录按钮-->\n          <b-nav-item v-if=\"$route.name !='login'\" @click=\"$router.replace({name:'login'})\">登录</b-nav-item>\n          <b-nav-item v-if=\"$route.name !='register'\" @click=\"$router.replace({name:'register'})\">注册</b-nav-item>\n        </div>\n      </b-navbar-nav>\n      </b-collapse>\n      </b-container>\n    </b-navbar>\n  </div>\n</template>\n<script>\nimport {mapState} from 'vuex';\nexport default {\n  computed: mapState({\n    // 从缓存中获取改为从vuex 中获取，因为vuex是响应式的，\n    userinfo: (state) => state.userModule.userInfo,\n    // return this.$store.state.userModule.userInfo;\n  }),\n};\n</script>\n<style lang=\"scss\" scoped></style>\n"]},"metadata":{},"sourceType":"module"}